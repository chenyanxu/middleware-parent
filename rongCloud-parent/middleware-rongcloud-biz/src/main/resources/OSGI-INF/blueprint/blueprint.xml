<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns:jpa="http://aries.apache.org/xmlns/jpa/v2.0.0"
           xmlns:tx="http://aries.apache.org/xmlns/transactions/v1.2.0"
           xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">
    <jpa:enable/>
    <tx:enable-annotations/>

    <reference id="tokenUserBeanDao" interface="com.kalix.middleware.rongcloud.api.dao.ITokenUserBeanDao"/>

    <!--融云服务注册-->
    <bean id="tokenUserBeanServiceImpl" class="com.kalix.middleware.rongcloud.biz.TokenUserBeanServiceImpl">
        <property name="dao" ref="tokenUserBeanDao"/>
    </bean>
    <service id="tokenUserBeanService" ref="tokenUserBeanServiceImpl"
             interface="com.kalix.admin.core.api.biz.IAddFieldService"/>

</blueprint>

